<body>
    <!--标题栏-->
    <header >
        <!--<a href="javascript:history.back();" class="back" ><img src="../../public/images/base/backArrow_ios.png"></a>-->
        <ul class="tabs f16 t_center">
            <li class="active"><a href="javascript:void (0)">原产优品</a></li>
            <li><a href="javascript:location.href = jumpUrl.clProList">定制生活</a></li>
        </ul>
    </header>

    <!--选择区域-->
    <div id="selArea">
        <div class="inner">

