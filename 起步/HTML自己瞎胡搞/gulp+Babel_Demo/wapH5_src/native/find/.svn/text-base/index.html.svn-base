                <ul class="selProvince"></ul>
            </div>

            <div class="innerRight">
                <ul class="selCity"></ul>
            </div>

        </div>
    </div>

    <!--首页顶部导航-->
    <nav>
        <div class="f_left find active">主题</div>
        <div class="line f_left"></div>
        <div class="swiper-container">
            <ul class="swiper-wrapper typeList">
                <li class='swiper-slide' typeid='0'>推荐</li>
            </ul>
        </div>
    </nav>


    <div id="wrapper">
        <div id="scroller">
            <!--下拉刷新-->
            <div id="pullDown">
                <span class="pullDownIcon"></span><span class="pullDownLabel">下拉刷新...</span>
            </div>

            <!--发现页面的内容-->
            <section class="content">
                <!--馆列表-->
                <ul class="guan"></ul>

                <!--爆款推荐-->
                <div class="baoKuanTuiJian"></div>
                <!--爆款推荐列表-->
                <ul class="baoKuan"></ul>

            </section>

            <!--一级菜单页面的内容-->

            <!--二级菜单-->
            <section class="content2">

                <ul class="typeList2 clearfix"></ul>

                <!--排序和选择地区-->
                <div class="select clearfix">
                    <div class="sort f_left">
                        <p zhpx="1">综合排序</p>
                        <ul>
                            <li zhpx="1">综合排序</li>
                            <li zhpx="2">人气排序</li>
                        </ul>
                    </div>

                    <div class="area f_left" regid="0" rregid="0">地区</div>
                </div>

                <!--商品列表-->
                <ul class="productsListArr"></ul>
                <div class="noData"><img src="../../public/images/base/noData.png"/></div>
            </section>

            <!--上拉加载-->
            <div id="pullUp">
                <span class="pullUpIcon"></span><span class="pullUpLabel">上拉加载更多...</span>
            </div>

        </div>
    </div>



    <footer>
        <ul class="clearfix">
            <li class="f_left"><a class="active home" href="javascript:void (0)" >商城</a></li>
            <li class="f_left"><a class=" me" href="javascript:location.href = jumpUrl.person" >我</a></li>
        </ul>
