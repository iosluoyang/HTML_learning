<script>
    //--------------------超级社区用户协议--------------------
    if(!localStorage.getItem("xieYi")){     //本地不存在用户协议
    $.ajax({
        url:getItemsUrl,
        data:getAuth()+"info={itemCode:3}",
        success:function(data){
            var list = data.resultdata.list;
            for(var i=0;i<list.length;i++){
                if(list[i].itemCode ==4){
                    localStorage.setItem("xieYi",list[i].itemValue);
                    break
                }
            }
        }
    });
    }
</script>
