<script>
    $(function(){
//        sessionStorage.setItem("completeUrl",jumpUrl.find);       //登录后跳转到首页
        isLogin(UID,function(){
            //退出 清除本地缓存
            $(".btn").click(function(e){
                e.preventDefault();
                var vid = sessionStorage.getItem("vid"); //缓存原产优品的vid
                localStorage.clear();
                sessionStorage.setItem("vid",vid);  //缓存原产优品的vid
                location.replace(jumpUrl.person);
            })
        })
    })
