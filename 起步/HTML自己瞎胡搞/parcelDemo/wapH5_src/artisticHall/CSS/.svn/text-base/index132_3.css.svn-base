/*蒙版*/
.artist .mengban{
    background-color: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    position: relative;
}

/*艺术家名称*/
.mengban .artistname{
    width: 100%;
    font-size: 1.7rem;
    color: #fff;
    position: absolute;
    left:50%;
    top:50%;
    transform: translate(-50%,-70%);
    -webkit-transform: translate(-50%,-70%);
    text-align: center;
}

/*作品字样*/
.mengban .zuopin{
    width: 100%;
    font-size: 1.2rem;
    color: #fff;
    position: absolute;
    left:50%;
    top:50%;
    transform: translate(-50%,50%);
    -webkit-transform: translate(-50%,50%);
    text-align: center;

}


/*作品分类区域*/
.productions_kind{
    background-image:
            url("../images/img8.png"),
            url("../images/bg1.png");
    background-repeat: no-repeat;
    background-position: 2.1rem 1.5rem,left top;
    background-size: 6.7rem 2rem,100% 10.2rem;
    background-color: #ebecec;

    display: -webkit-box;

    display: -ms-flexbox;

    display: flex;
    display:-webkit-flex;
    -webkit-box-pack: justify;
        -ms-flex-pack: justify;
            justify-content: space-between;
    -webkit-justify-content: space-between;
    -ms-flex-wrap: wrap;
        flex-wrap: wrap;
    -webkit-flex-wrap: wrap;
    padding: 8.2rem 6.7% 0 6.7%;
}
/*作品分类*/
.productkind {
    width: 22.6%;
    background-repeat: no-repeat;
    background-size: cover;
}

/*精品推荐*/
.sample_reels{
    background-image:
            url("../images/img9.png"),
            url("../images/bg2.png");
    background-repeat: no-repeat;
    background-position:2.1rem 4.7rem , left bottom;
    background-size: 6.75rem 1.9rem,100% 21.3rem;
    background-color: #ebecec;
    padding: 9.9rem 1rem 21.3rem 1rem;
}

/*精品产品*/
.product{
    background-color: #ffffff;
    background-repeat: no-repeat;
    background-position: center top;
    border-radius: 0.4rem;
    float: left;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    margin-bottom:1rem;
}
/*第135等奇数项有margin-right，其他的都没有*/
.product:nth-child(odd){
    margin-right: 6px;
}

/*精品推荐名称*/
.product .productname{
    color: #000;
    font-size:1.2rem;
    margin:1rem 1.35rem 1.75rem 1.35rem ;

}
.btnmore{
    display: block;
    margin: 0 auto;
    margin-top: 2.5rem;
    width: 15.8rem;
}
